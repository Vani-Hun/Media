<script>
    const customer = JSON.parse('<%- JSON.stringify(customer) %>');

    let videoForm = document.querySelector('.jsx-2834184006');
    let videoFormBefore = document.querySelector('.jsx-2977532431.uploader');
    let selectFileButton = videoForm.querySelector('input[type="file"]');
    let videoFormaAfter = document.querySelector('.jsx-2977532431.preview');
    let changeButton = document.querySelector('.jsx-3536812553.btn')
    let thumbnailEmpty = document.querySelector('.candidate-v2');
    let thumbnailContainer = document.querySelector('.bg-container-v2');
    let selector = document.querySelector('.tiktok-select-selector');
    let dropdown = document.querySelector('.tiktok-select-dropdown');
    let checkboxes = document.querySelectorAll('.css-1pzrh5a');
    // Duyệt qua từng checkbox và thêm sự kiện "change"
    checkboxes.forEach(function (checkbox) {
        checkbox.addEventListener('change', function (event) {
            let icon = document.querySelector('.css-mbgljv');
            let icon2 = document.querySelector('.css-3wpmrz');


            // Kiểm tra xem checkbox đã được chọn hay không
            if (icon) {
                // Nếu được chọn, thay đổi class thành 'css-mbgljv'
                icon.classList.remove('css-mbgljv');
                icon.classList.add('css-3wpmrz');
            } else if (icon === null) {
                // Nếu không được chọn, thay đổi class thành 'css-3wpmrz'
                icon2.classList.remove('css-3wpmrz');
                icon2.classList.add('css-mbgljv');
            }
        });
    });
    let isDropdownVisible = false;
    selector.addEventListener('click', function () {
        if (isDropdownVisible) {
            dropdown.style.display = 'none';
        } else {
            dropdown.style.display = 'block';
        }

        isDropdownVisible = !isDropdownVisible;
    });
    let dropdownItems = document.querySelectorAll('.tiktok-select-dropdown-item');
    dropdownItems.forEach(function (item) {
        item.addEventListener('click', function () {
            selector.textContent = item.textContent;
            dropdown.style.display = 'none';
            isDropdownVisible = false;
        });
    });
    videoForm.addEventListener('click', function (event) {
        if (event.target !== selectFileButton) {
            selectFileButton.click();
        }
    });
    changeButton.addEventListener('click', function (event) {
        window.location.href = '/customer/upload'
    });

    function addFrameToCover(frameDataURL) {
        thumbnailEmpty.style.display = 'none';
        let frameImage = document.createElement('img');
        frameImage.src = frameDataURL;
        frameImage.className = 'candidate-v2 bg';
        frameImage.draggable = false;
        frameImage.addEventListener('click', function () {
            let postButton = document.querySelector('.jsx-3366794632.btn-post button');
            postButton.removeAttribute('disabled');
            let images = document.querySelectorAll('.candidate-v2.bg');
            images.forEach(function (image) {
                image.classList.remove('selected-image');
            });
            frameImage.classList.add('selected-image');
        });
        thumbnailContainer.appendChild(frameImage);
    }

    let selectedFile;
    selectFileButton.addEventListener('change', function (event) {
        selectedFile = event.target.files[0];

        if (selectedFile) {

            videoFormBefore.style.display = 'none';
            videoFormaAfter.style.display = 'block';

            let video = document.createElement('video');
            let canvasCapture = document.createElement('canvas');
            let ctxCapture = canvasCapture.getContext('2d');

            let canvas = document.getElementById("mobile-preview-canvas");
            let video2 = document.createElement('video');
            video2.src = URL.createObjectURL(selectedFile);
            video2.onloadedmetadata = function () {
                // Khi video2 đã tải xong, thiết lập kích thước cho canvas
                canvas.width = video2.videoWidth;
                canvas.height = video2.videoHeight;
                let ctx = canvas.getContext("2d");

                // Hàm vẽ video lên canvas
                function drawVideoFrame() {
                    ctx.drawImage(video2, 0, 0, canvas.width, canvas.height);
                    requestAnimationFrame(drawVideoFrame);
                }

                // Bắt đầu vẽ video lên canvas bằng cách gọi hàm đầu tiên
                drawVideoFrame();
            };
            video2.addEventListener('ended', function () {
                // Khi video kết thúc, đặt thời gian chơi về 0 và chơi lại video
                this.currentTime = 0;
                this.play();
            });
            // Phát video2
            video2.play();


            let reader = new FileReader();

            reader.onload = function (event) {
                video.src = event.target.result;
                video.onloadedmetadata = function () {
                    canvasCapture.width = video.videoWidth;
                    canvasCapture.height = video.videoHeight;
                    let videoDuration = video.duration;
                    let numFrames = 6;
                    let interval = parseFloat((videoDuration / numFrames).toFixed(2));
                    let captureFrames = function (currentTime) {
                        video.currentTime = currentTime;
                        video.onseeked = function () {
                            ctxCapture.drawImage(video, 0, 0, canvasCapture.width, canvasCapture.height);
                            let frameDataURL = canvasCapture.toDataURL('image/jpeg', 0.5);
                            addFrameToCover(frameDataURL);
                            if (currentTime + interval <= videoDuration) {
                                captureFrames(currentTime + interval);
                            } else {
                            }
                        };
                    };

                    captureFrames(0);

                };
            };

            // Đọc dữ liệu từ tệp video được chọn
            reader.readAsDataURL(selectedFile);
        }
    });
    const textBox = document.querySelector('.public-DraftEditor-content');

    textBox.addEventListener('input', function (event) {
        let inputValue = event.target.innerText.trim();
        const MAX_CHARACTERS = 50; // Số kí tự tối đa cho phép
        if (inputValue.length > MAX_CHARACTERS) {
            // Cắt bớt văn bản để chỉ chứa số kí tự tối đa cho phép
            inputValue = inputValue.substring(0, MAX_CHARACTERS);
            event.target.innerText = inputValue;
        }
    });

    // Lấy tham chiếu đến nút "Post"
    let postButton = document.querySelector('.jsx-3366794632.btn-post button');
    function showLoader() {
        let loader = document.querySelector('.loader');
        loader.classList.add('visible');
    }
    function hideLoader() {
        let loader = document.querySelector('.loader');
        loader.classList.remove('visible');
    }
    // Lắng nghe sự kiện click trên nút "Post"
    postButton.addEventListener('click', function () {
        showLoader()
        // Gọi hàm gửi API khi nút "Post" được nhấn
        sendApiRequest();
    });

    // Hàm gửi API
    function sendApiRequest() {

        let checkbox = document.querySelector('.css-mbgljv')
        let allowUsers;
        let selectedImageSrc = document.querySelector('.selected-image').src;

        if (checkbox) {
            allowUsers = true;
        } else { allowUsers = false; }

        let formData = new FormData();
        formData.append('video', selectedFile);
        formData.append('caption', textBox.textContent.trim());
        formData.append('image', selectedImageSrc);
        formData.append('allowComment', allowUsers);
        formData.append('who', selector.textContent.trim());

        // Sử dụng fetch để gửi yêu cầu POST
        fetch('/video/upload', {
            method: 'POST',
            body: formData
        })
            .then((data) => {
                hideLoader()
                window.location.href = `/customer/profile/${customer.id}`
            })
            .catch(error => {
                // Xử lý lỗi nếu có
                console.error('There has been a problem with your fetch operation:', error);
            });
    }


</script>