    function handleTabClick(clickedButton) {
        buttons.forEach(button => {
            if (button !== clickedButton) {
                button.classList = 'css-7p3g5e-ButtonGroupItem'
            }
        });

        clickedButton.classList = 'css-4p4iga-ButtonGroupItem'
        listNotifications.innerHTML = ''

        customer.notifications.forEach(notification => {
            if (notification.type === clickedButton.innerHTML) {
                const notificationElement = createNotificationElement(notification);
                listNotifications.appendChild(notificationElement);
            }
            if (clickedButton.innerHTML.trim() === "All activity") {
                const notificationElement = createNotificationElement(notification);
                listNotifications.appendChild(notificationElement);
            }
        })

    }
    function hideNotifications() {
        if (!inboxIcon.contains(event.target) && !notifications.contains(event.target)) {
            notifications.style.display = "none";
            document.removeEventListener('click', hideNotifications);
        }
    };
    function showDropdownMenu() {
        clearTimeout(timeSet);
        if (dropdownMenu.style.display === "none") {
            notifications.style.display = "none";
            dropdownMenu.style.display = "block";
        }
    };
    function hideDropdownMenu(event) {
        timeSet = setTimeout(() => {
            dropdownMenu.style.display = "none";
            document.removeEventListener('click', hideDropdownMenu);
        }, 700)
    };
    inboxIcon.addEventListener('click', (event) => {
        if (notifications.style.display === "none") {
            dropdownMenu.style.display = "none";
            notifications.style.display = "block";
            numberNotification.style.display = 'none'
            document.addEventListener('click', hideNotifications);
            event.stopPropagation();
            fetch(`/notification/check`, {
                method: 'POST',
            })
                .then(data => {
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        } else {
            notifications.style.display = "none";
        }
    })
    openMessages.addEventListener('click', (event) => {
        const containers = [
            document.querySelector('.css-1t4vwes-DivMainContainer'),
            document.querySelector('.tiktok-14dcx2q-DivBodyContainer'),
            document.querySelector('.css-14dcx2q-DivBodyContainer'),
        ];

        for (let i = 0; i < containers.length; i++) {
            if (containers[i]) {
                mainMessages.style.display = 'flex';
                navMenuMiddle.style.display = 'none';
                containers[i].style.display = 'none';
                break;
            }
        }
    });

    profileIcon.addEventListener("mouseenter", function (event) {
        if (notifications.style.display === "none") {
            showDropdownMenu()
        }
    });
    dropdownMenu.addEventListener("mouseleave", hideDropdownMenu);
    dropdownMenu.addEventListener("mouseover", showDropdownMenu);
    document.addEventListener('click', hideDropdownMenu);
    if (listItem) {
        listItem.onclick = function () {
            window.location.href = "/customer/log-out";
        };
    }
    listItems.forEach(function (listItem, index) {
        let link = listItem.querySelector('a');
        if (link) {
            const hrefPaths = ['/customer/profile', '/customer/setting', '/customer/feedback'];
            if (index < hrefPaths.length) {
                listItem.onclick = function () {
                    if (index == 0) {
                        window.location.href = `${hrefPaths[index]}/${customer.id}`;
                    } else {
                        window.location.href = `${hrefPaths[index]}`;
                    }

                };
            }
        }
    })

    if (customer !== null && customer.logo !== null && customer.logo !== "") {
        profileIcon.style = `background-image:url( ${customer.logo} )`
        let countNoti = 0
        customer.notifications.forEach(notification => {
            const notificationElement = createNotificationElement(notification);
            listNotifications.appendChild(notificationElement);
            if (notification.status === false) {
                countNoti++
            }
        })
        if (countNoti > 0) {
            numberNotification.innerHTML = countNoti
            numberNotification.style.display = 'flex'
        }
    } else if (customer === null) {
        iconNotifications.style.display = 'none'
        iconMess.style.display = 'none'
        profileIcon.style.display = 'none'
    }
    else {
        profileIcon.style = "background-image:url(https://w0.peakpx.com/wallpaper/331/769/HD-wallpaper-lazy-egg5-egg-gudetama-kawaii-lazy-egg.jpg)";
    }



</script>