<script>
    let cursor = JSON.parse('<%- JSON.stringify(cursor) %>') || null;
    const listItemsMidleNav = document.querySelectorAll('.css-nwhccz-UlMainNav > li');

    listItemsMidleNav.forEach(item => {
        let tagSpan = item.querySelector('span');
        let tagSVG = item.querySelector('svg');

        if (!tagSpan || !tagSVG) return;

        switch (cursor) {
            case "Following":
                if (tagSpan.innerText === "Following") {
                    tagSVG.setAttribute("fill", "rgba(254, 44, 85, 1)");
                    tagSpan.classList.add('css-35pnuj-SpanMainNavText');
                }
                break;
            case "Friends":
                if (tagSpan.innerText === "Friends") {
                    tagSVG.setAttribute("fill", "rgba(254, 44, 85, 1)");
                    tagSpan.classList.add('css-35pnuj-SpanMainNavText');
                }
                break;
            case "For You":
                if (tagSpan.innerText === "For You") {
                    tagSVG.setAttribute("fill", "rgba(254, 44, 85, 1)");
                    tagSpan.classList.add('css-35pnuj-SpanMainNavText');
                }
                break;
            default:
                break;
        }
    });



    const navMenuMiddle = document.querySelector('.css-r0hg2a-DivSideNavContainer')
    const buttonShowMoreText = document.querySelector('.css-b1mrwj-ButtonShowMoreText p')
    let initialItems = 5;
    let itemsToAdd = 3;

    let ulList = document.querySelector('.css-qrbz7k-DivUserContainer');
    let allItems = ulList.querySelectorAll('ul');

    for (let i = initialItems; i < allItems.length; i++) {
        allItems[i].style.display = 'none';
    }

    buttonShowMoreText.addEventListener('click', () => {
        if (buttonShowMoreText.innerText == 'See less') {
            initialItems = 5;
            for (let i = initialItems; i < allItems.length; i++) {
                allItems[i].style.display = 'none';
            }
            buttonShowMoreText.innerText = 'See more';
        } else {
            for (let i = initialItems; i < initialItems + itemsToAdd && i < allItems.length; i++) {
                allItems[i].style.display = 'list-item';
            }
            initialItems += itemsToAdd;
            if (initialItems >= allItems.length) {
                buttonShowMoreText.innerText = 'See less';
            }
        }
    })
</script>